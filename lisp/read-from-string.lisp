(in-package "SYSTEM")

(defun read-from-string (string &optional (eof-error-p t) eof-value
                                &key (start 0) end preserve-whitespace)
  (declare (optimize speed))
  (%read-from-string string eof-error-p eof-value start end preserve-whitespace))
